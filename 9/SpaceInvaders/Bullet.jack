class Bullet { 
    field int x, y;

    constructor BulletConstructor new(int argX, int argY) {
        this.x = argX;
        this.y = argY;
        return this;
    }

    method void draw() {
        var int memAddress; 
        let memAddress = 16384 + this.y * 32 + this.x / 16;
        // column 0
        do Memory.poke(memAddress, 1);
        do Memory.poke(memAddress +32, 1);
        return;
    }
}